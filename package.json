{
  "name": "philonutri-lab",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx server/index.ts\" \"vite\"",
    "build": "vite build && npx tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint src server --ext .ts,.tsx",
    "lint:fix": "eslint src server --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\" \"server/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\" \"server/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "docker:build": "docker build -t philonutri-lab .",
    "docker:run": "docker run -p 3001:3001 --env-file .env.local philonutri-lab",
    "prepare": "echo 'Ready to commit'"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "helmet-csp": "^4.0.0",
    "i18next": "^23.10.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.7.1",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}